{
  "name": "cpycnf",
  "version": "0.0.0",
  "description": "Define recurring file copy tasks in a config and execute them.",
  "license": "MIT",
  "keywords": [
    "config",
    "copy",
    "cpy",
    "file",
    "files",
    "glob"
  ],
  "homepage": "https://github.com/herteleo/cpycnf#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/herteleo/cpycnf.git"
  },
  "author": {
    "name": "Leonard Hertel",
    "email": "leonard.hertel@gmail.com",
    "url": "https://herteleo.de"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "bin": {
    "cpycnf": "dist/cli.js"
  },
  "files": [
    "dist/cli.js"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup --minify",
    "lint": "eslint ."
  },
  "dependencies": {
    "cpy": "^10.1.0",
    "lilconfig": "^2.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.6",
    "@tsconfig/esm": "^1.0.3",
    "@tsconfig/node20": "^1.0.1",
    "@types/node": "^20.3.2",
    "eslint": "^8.43.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.3"
  },
  "eslintConfig": {
    "extends": "@antfu"
  },
  "tsup": {
    "entry": [
      "src/cli.ts"
    ],
    "format": "esm",
    "clean": true
  }
}
